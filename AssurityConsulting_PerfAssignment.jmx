            <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="JSR223 PostProcessor" enabled="true">
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="script">import groovy.json.JsonSlurper
import groovy.json.JsonParserType
import static com.jayway.jsonpath.JsonPath.parse

parser = new JsonSlurper().setType(JsonParserType.INDEX_OVERLAY);

String categoryId=vars.get(&quot;c_CategoryID&quot;);
String name=vars.get(&quot;c_Name&quot;);
String path=vars.get(&quot;c_Path&quot;);
String promotion=&quot;[&quot;+vars.get(&quot;c_Promotion&quot;)+&quot;]&quot;;
String filepath=vars.get(&quot;CSV_FilePath&quot;);

if (promotion!=&quot;[NOT_FOUND]&quot;)
{
	String constantData=categoryId+&quot;,&quot;+name+&quot;,&quot;+path;
	
	promotion_object = parser.parseText(promotion);
	
	promotion_collection_count = parse(promotion_object).read(&apos;$.*&apos;).size();
	
	//log.info(&quot;Count &quot;+promotion_collection_count);
	
	FileWriter f = new FileWriter(&quot;&quot;+filepath+&quot;/CSV_Output.csv&quot;,true);
	BufferedWriter b = new BufferedWriter(f);

	for (int i=0;i&lt;promotion_collection_count;i++){
		//log.info(constantData+&quot;,&quot;+promotion_object[i].Id+&quot;,&quot;+promotion_object[i].Price);
		b.write(constantData+&quot;,&quot;+promotion_object[i].Id+&quot;,&quot;+promotion_object[i].Price+&quot;\n&quot;);
	}
			
	b.flush();
	b.close();
}</stringProp>
